[Unit]
Description=FastAPI App para Taller S.O.
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/Final_FastAPI
ExecStart=/home/ubuntu/Final_FastAPI/venv/bin/uvicorn main:app --host 0.0.0.0 --port 8000
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
